    def nthUglyNumber(self, n):
        u=[1]
        i2,i3,i5=0,0,0
        while len(u)<n:
            nxt2,nxt3,nxt5=u[i2]*2,u[i3]*3,u[i5]*5
            nu=min(nxt2,nxt3,nxt5)
            u.append(nu)
            if nu==nxt2:
                i2+=1
            if nu==nxt3:
                i3+=1
            if nu==nxt5:
                i5+=1
        return u[-1]
