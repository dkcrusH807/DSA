# Write Python 3 code in this online editor and run it.
def nqueen(n):
    res=[]
    cols=set()
    posdiag=set()
    negdiag=set()
    b=[["."]*n for _ in range(n)]
    def back(r):
        if r==n:
            res.append(["".join(r) for r in b])
            return
        for c in range(n):
            if c in cols or (r+c) in posdiag or (r-c) in negdiag:
                continue
            
            cols.add(c)
            posdiag.add(r+c)
            negdiag.add(r-c)
            b[r][c]="Q"
            back(r+1)
            cols.remove(c)
            posdiag.remove(r+c)
            negdiag.remove(r-c)
            b[r][c]="."
            
    back(0)
    return res
print(nqueen(4))
sol=nqueen(4)
for s in sol:
    for r in s:
        print(r)
    print()
    
