    def addBinary(self, a, b):
        i,j=len(a)-1,len(b)-1
        res=[]
        c=0
        while i>=0 or j>=0 or c:
            t=c
            if i>=0:
                t+=int(a[i])
                i-=1
            if j>=0:
                t+=int(b[j])
                j-=1
            res.append(str(t%2))
            c=t//2
        return "".join(reversed(res))
