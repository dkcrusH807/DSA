    def canPlaceFlowers(self, flowerbed, n):
        for i in range(len(flowerbed)):
            if flowerbed[i]==0:
                eleft=(i==0) or (flowerbed[i-1]==0)
                eright=(i==len(flowerbed)-1) or (flowerbed[i+1]==0)
                if eleft and eright:
                    flowerbed[i]=1
                    n-=1
                    if n==0:
                        return True
        return n<=0
